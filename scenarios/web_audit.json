{
  "name": "Web Service Audit",
  "steps": [
    {
      "tool": "nmap",
      "args": ["-sV", "192.168.1.10"]
    },
    {
      "tool": "gobuster",
      "args": ["dir", "-u", "http://192.168.1.10", "-w", "/usr/share/wordlists/dirb/common.txt"]
    },
    {
      "tool": "nikto",
      "args": ["-h", "http://192.168.1.10"]
    },
    {
      "tool": "sqlmap",
      "args": ["-u", "http://192.168.1.10/login.php?user=admin&pass=123", "--batch"]
    }
  ]
}
